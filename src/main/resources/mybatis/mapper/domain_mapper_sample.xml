<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="domain_mapper_sample">
	<select id="searchAllFoodItem" resultMap="foodItem">
		SELECT
			item.ID AS foodCode,
			item.NAME AS foodName
		FROM
			Item item
	</select>

	<resultMap type="com.github.apz.springsample.domain.model.FoodItem" id="foodItem">
		<result column="foodCode" property="foodCode" typeHandler="com.github.apz.springsample.domain.typehandler.DomainValueTypeHandler" />
		<result column="foodName" property="foodName"/>
	</resultMap>
</mapper>